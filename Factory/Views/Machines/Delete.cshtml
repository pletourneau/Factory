@{
  Layout = "_Layout";
}

@model Factory.Models.Machine;

<h2>Are you sure you want to delete this Machine?</h2>
<h4>Machine: @Html.DisplayFor(model => model.MachineName)</h4> 
@using (Html.BeginForm())
{
  <input type="submit" value="X" class="btn btn-outline-danger btn-sm" />
}
<div class="container">
  <h5>@Html.ActionLink("Back to Machines", "Index")</h5>
</div>